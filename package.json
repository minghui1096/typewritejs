    "@types/raf": "^3.4.3",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.41.0",
    "rollup-plugin-dts": "^6.2.1",
    "tslib": "^2.8.1",
